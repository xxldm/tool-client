    "@vueuse/sound": "^1.1.7",
    "axios": "^1.1.2",
    "electron-updater": "^5.3.0",
    "element-plus": "^2.2.17",
    "github-markdown-css": "^5.1.0",
    "lunar-fun": "^2.0.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.23",
    "vue": "^3.2.40",
    "vue-i18n": "^9.2.2",
