  },
  "main": "dist/electron/main/index.js",
  "engines": {
    "node": ">=14.17.0"
  },
  "scripts": {
    "dev": "vite --port 8080",
    "build": "vite build && electron-builder",
    "build:dir": "vite build && electron-builder --dir",
    "build:vite": "vite build",
    "build:electron": "electron-builder",
    "preview": "vite preview",
    "preview:build:base": "vite build --base %npm_config_base% && vite preview --base %npm_config_base%",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest",
    "up": "taze major -I",
    "tag": "bumpp --execute \"conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md\" --all"
  },
